<html>
    <!-- based on no-side bar template -->
    <%- include('../partials/head'); %>
    <body class="is-preload">
		<div id="page-wrapper">

			<!-- Header -->
			<%- include('../partials/header'); %>

            <script src="/assets/js/Chart.js"></script>
            <script type="text/javascript" src="https://unpkg.com/vis-network/standalone/umd/vis-network.min.js"></script>

			<!-- Main -->
				<section class="wrapper style1">
					<div class="container">
						<div id="content">
							<!-- Content -->
                            <article>
                                <header>
                                    <h2><%= webcontent.main.module %></h2>
                                    <%- include('../partials/pluginstats'); %>
                                    <p><%= webcontent.main.introduction %></p>
                                    <p>
                                        <b><%= webcontent.main.note1 %>:</b> <%= webcontent.main.note2 %>
                                    </p>
                                </header>
                                <p>
                                    <% if(catalog.length > 0) { %>
                                        <div class="container">
                                            <div class="row gtr-200">
                                                <div class="col-4 col-12-narrower" style="width: 600px; height: 400px;">
                                                    <%- include('../partials/portfolio_analytics/ColPortfolioStatus'); %>
                                                </div>    
                                                <div class="col-4 col-12-narrower" style="width: 600px; height: 400px;">
                                                    <%- include('../partials/portfolio_analytics/ColPortfolioNewIssues'); %>
                                                </div>    
                                            </div>
                                            <p>&nbsp;</p>
                                            <div class="row gtr-200">    
                                                <div class="col-4 col-12-narrower" style="width: 600px; height: 400px;">
                                                    <%- include('../partials/portfolio_analytics/ColPortfolioRiskRating'); %>
                                                </div>    
                                                <div class="col-4 col-12-narrower" style="width: 600px; height: 400px;">
                                                </div>    
                                                <p>&nbsp;</p>
                                            </div>
                                        </div>
                                        <hr/>
                                        <section class="wrapper style1">
                                            <div class="container">
                                                <%- include('../partials/portfolios'); -%>
                                            </div>
                                        </section>
                                    <% } else { %>
                                        <p><%= webcontent.main.noaudits %></p>
                                    <% } %>
                                </p>
                            </article>
						</div>
					</div>
				</section>

			<!-- Footer -->
			<%- include('../partials/footer'); %>

		</div>

		<!-- Scripts -->
		<%- include('../partials/scripts'); -%>

	</body>
</html>